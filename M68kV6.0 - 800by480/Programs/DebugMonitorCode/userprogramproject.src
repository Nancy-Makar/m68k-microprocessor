; register int i = (xtod(one) << 24) | (xtod(two) << 20) | (xtod(three) << 16) | (xtod(four) << 12) | (xtod(five) << 8) | (xtod(six) << 4) | (xtod(seven));
       move.b    11(A6),D1
       ext.w     D1
       ext.l     D1
       move.l    D1,-(A7)
       jsr       (A2)
       addq.w    #4,A7
       and.l     #255,D0
       asl.l     #8,D0
       asl.l     #8,D0
       asl.l     #8,D0
       move.l    D0,-(A7)
       move.b    15(A6),D0
       ext.w     D0
       ext.l     D0
       move.l    D0,-(A7)
       jsr       (A2)
